// Example-Template:
parens(x) ::= << (( $x$ )) >>

pair(x,y) ::= << (( $x$, $y$ )) >>

nested(elems) ::= <<
nested {
    $elems:parens();separator=",\n"$;
};
>>

nested2(name, helems) ::= <<
nested2 "<name>" { $helems:{e|
    $e.name$ $e.type:parens()$$if(e.initval)$ = $e.initval$$endif$};separator=";"$;
};
>>

nested3(name, names, types) ::= <<
nested3 "$name$" {
$names,types:{n,t | $pair(n,t)$}$
};
>>

nested4(name, helems) ::= <<
nested4 "$name$" {
  $ helems:{e | $pair(e.name,e.type)$};separator="\n" $
};
>>
